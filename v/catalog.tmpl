<main>
	{% if catalog isset %}
    {% foreach catalog as product %}
      <a href="index.php?c=catalog&act=product&id={{ product.id_product }}"><img src="{{ product.image }}" alt="{{ product.name }}"></a>
      <h3>{{ product.name }}</h3>
      <h4>Цена: {{ product.price }}</h4>
    {% endforeach %}
  {% endif %}
</main>